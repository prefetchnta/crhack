##
##=====================================
##      公用定义项
##=====================================
##
L=/
C=c
O=doj
P=cpp

##
##=====================================
##      附加的参数
##=====================================
##
include defs.inc
include conf.inc

##
##=====================================
##      命令行工具
##=====================================
##
CC=ccblkfn.exe
PP=ccblkfn.exe -c++
AR=elfar.exe
LD=linker.exe

###############################################################################
##                             Blackfin (NAKED)                              ##
###############################################################################

##
##=====================================
##      编译命令行公用参数
##=====================================
##
CFLAGS=-c -O1 -Ov100 -structs-do-not-overlap -no-multiline \
       -decls-strong -warn-protos -I ..\inc $(C_ADDONS)

##
##=====================================
##      静态库生成公用参数
##=====================================
##
AFLAGS=-c $(A_ADDONS) $(LIB_NAME)

